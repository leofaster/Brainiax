declare foo, bar :: integer execute
    bar := 3 ;
    $- Asignar a foo
    el valor 35 + bar, si bar > 2. -$
    read bar ;
    $-write "ESTO, Es una, cadena de cosas raras %$^*" Problemas con el write-$
    if bar > 2 then
        foo := 35 + bar
    done;
    for foo from 0 to 7 do
        bar := bar + 1
    done;
    while foo < 4 do
        foo := foo + 1
    done
done

