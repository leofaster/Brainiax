declare x, y, z:: integer; t, s, p :: tape; b, c, d:: boolean execute 
   x := y + z;
   t := s & p;
   b := c /\ d;
   write a;
   read x;
   for i from 1 to x do
        {++++++++++} at t;

        while y /= 0 do 
            if x>y then
                declare y :: integer execute
                    y := 4;
                    z := 1
                done
            done;
            {>+++++++>++++++++++>+++>+<<<<-} at t
        done;
        {>++.>+.+++++++..+++.>++.} at t;
        {<<+++++++++++++++.>.+++.} at s;
        x := -x
    done
done
